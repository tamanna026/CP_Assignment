//Problem 3

#include<bits/stdc++.h>
using namespace std;
int fo(int arr[],int n,int x)
{
    int low=0,high=n-1;
    int ans=-1;
    while(low<=high)
    {
        int mid=low+(high-low)/2;

        if(arr[mid]==x)
        {
            ans=mid;
            high=mid-1;
        }
        else if(arr[mid]<x)
        {
            low=mid+1;
        }
        else
        {
            high=mid-1;
        }
    }
    return ans;
}
int lo(int arr[],int n,int x)
{
    int low=0,high=n-1;
    int ans=-1;
    while(low<=high)
    {
        int mid=low+(high-low)/2;
        if(arr[mid]==x)
        {
            ans=mid;
            low=mid+1;
        }
        else if(arr[mid]<x)
        {
            low=mid+1;
        }
        else
        {
            high=mid-1;
        }
    }
    return ans;
}
int main()
{
    int n;
    cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
        cin>>arr[i];
    }
    int x;
    cin>>x;
    int a=fo(arr,n,x);
    int b=lo(arr,n,x);
    cout<<a<<" "<<b<<endl;
}
