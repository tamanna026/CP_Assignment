//Problem 2

#include<bits/stdc++.h>
using namespace std;


int lowerbounder(int arr[], int n, int target)
{
   int low=0; int high=n-1;
   int ans=n;
   while (low<=high)
   {
      int mid=low+(high-low)/2;

      if(arr[mid]>=target)
      {
         ans=mid;
         high=mid-1;//left
      }
      else{

        low=mid+1;//rigth
      }

   }

   return ans;
   

    
}

int main()
{

    
    int n;cin>>n;
    int arr[n];
    for(int i=0;i<n;i++)
    {
       cin>>arr[i];
    }

    int target;cin>>target;
  
    cout<<lowerbounder(arr,n,target);

    return 0;
}
